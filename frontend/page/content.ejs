<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="./../css/nav.css">
    <link rel="stylesheet" href="./../css/content.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css"
        integrity="sha512-SnH5WK+bZxgPHs44uWIX+LLJAJ9/2PkPKZ5QiAj6Ta86w+fsb2TkcmfRyVX3pBnMFcV7oQPJkl9QevSCWr3W6A=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
        <title>SIT Blog</title>
        <link rel="icon" href="../1.png">
</head>
<body>
    <div>
        <%- nav %>
        <div class="empty" style="height: 136px;"></div>
        <% putContent.forEach(function(content) { %>
        <div class="page">
            <div class="box-content">
                <div class="wrap-all-content">
                    <div class="wrap-content-content1">
                        <div class="wrap-content-picture">
                            <img src="<%= content.urlpicture %>" alt="Picture">
                        </div>
                        <div class="wrap-content-text">
                            <div class="heading-text"><%= content.postTitle %></div>
                            <div class="desc-text" ><p><%= content.description %></p></div>
                        </div>
                        <div class="wrap-content-text2">
                            <div class="name-text"><%= content.name %> (posted)</div>
                            <div class="date-text"><%= content.editDate %></div>
                        </div>
                    </div>
                    <div class="wrap-content-content2">
                        <div class="wrap-tag-content">
                            <% if (content.typeSIT) { %><div class="tag-sit">SIT</div><% } %>
                            <% if (content.typeIT) { %><div class="tag-it">IT</div><% } %>
                            <% if (content.typeCS) { %><div class="tag-cs">CS</div><% } %>
                            <% if (content.typeDSI) { %><div class="tag-dsi">DSI</div><% } %>
                        </div>
                        
                        <form action="/likeReturn?idPost=<%= content.idPost %>" method="get" class="wrap-heart-content">
                            <div style="display: flex; align-items: center;">
                                <input type="checkbox" name="like" id="checkboxheart" value="like">
                                <label for="checkboxheart" class="heart">
                                    <i class="fa-solid fa-heart" id="heart" onclick="Like()"></i>
                                </label>
                            </div>
                            <input type="hidden" name="idPost" value="<%= content.idPost %>">
                            <input type="submit" value="Back to Blogs" class="text-back-content">
                        </form>

                        <script>
                                let num = 2;
                                function Like() {
                                    const heart = document.querySelector('.fa-heart');
                                    heart.classList.toggle('heartToggle');
                                    
                                    const checkbox = document.getElementById('checkboxheart');
                                    if (num % 2 === 0) checkbox.checked = false; // ตั้งให้ checkbox เป็นเช็ค
                                    else checkbox.checked = true;
                                    num++;
                                    console.log("Checkbox checked status:", !checkbox.checked);
                                }

                        </script>
                        
                    </div>
                    <% if (String(content.idAuthor).includes(userID)) { %>
                        <div class="wrap-link-text" >
                            <div class="delete-text" id="logout"  onclick="logoutPopup()" >Delete</div>
                            <a href="/toeditContent?idPost=<%= content.idPost %>" class="edit-text">Edit</a>
                        </div>
                    <% } %>  
                </div>
            </div>
        </div>
        <div class="popup" id="popup">
            <div class="logo">
                <i class="fa-solid fa-trash"></i>
            </div>
            <div class="text"><span>Are you sure you want to Delete?</span></div>
            <div class="popupBtn">
                <a href=""><input type="button" onclick="closePopup()" value="cancel" id="cancel" class="cancel" /></a>
                <a href="/deleteContent?idPost=<%= content.idPost %>"><input type="button" onclick="closePopup()" value="delete" id="logout"
                        class="logout" /></a>
