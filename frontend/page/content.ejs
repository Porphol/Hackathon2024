    </style>
    <script>
        logout = document.getElementById("logout");
        cancelBtn = document.getElementById("cancel");
        popup = document.getElementById('popup');
        close = document.getElementById('open');
        content = document.getElementById('content');
        nav = document.getElementById('nav');
        contentPost = document.getElementById('contentPost');
        contentLike = document.getElementById('contentLike');

        function logoutPopup() {
            popup.classList.add('open');
            content.style.filter = "blur(5px)";
            nav.style.filter = "blur(5px)";
        }
        function closePopup() {
            content.style.filter = "none";
            nav.style.filter = "blur(5px)";
        }

        function changePageToLike() {
            contentPost.style.display = "none";
            contentLike.style.display = "flex";
        }
        function changePageToPost() {
            contentPost.style.display = "flex";
            contentLike.style.display = "none";
        }
    </script>
